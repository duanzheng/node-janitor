{
  "name": "node-janitor",
  "productName": "node-janitor",
  "version": "1.0.6",
  "description": "A powerful desktop application for cleaning Node.js project dependencies and package manager caches",
  "main": ".webpack/main",
  "scripts": {
    "dev": "cross-env NODE_ENV=development electron-forge start",
    "dev:restart": "yarn clean && yarn dev",
    "clean": "rm -rf dist .webpack",
    "build": "webpack --config webpack.renderer.config.js",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make:universal": "electron-forge make --arch=universal",
    "publish": "electron-forge publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "prepare": "husky",
    "lint-staged": "lint-staged",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "electron",
    "nodejs",
    "node-modules",
    "cache",
    "cleanup",
    "package-manager",
    "npm",
    "yarn",
    "pnpm",
    "desktop-app"
  ],
  "author": {
    "name": "TonyDuan",
    "email": "duanzheng21518@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "antd": "^5.26.7",
    "electron-squirrel-startup": "^1.0.1",
    "execa": "^9.6.0",
    "fs-extra": "^11.3.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@electron-forge/cli": "^7.8.3",
    "@electron-forge/maker-deb": "^7.8.3",
    "@electron-forge/maker-dmg": "^7.8.3",
    "@electron-forge/maker-rpm": "^7.8.3",
    "@electron-forge/maker-squirrel": "^7.8.3",
    "@electron-forge/maker-zip": "^7.8.3",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.3",
    "@electron-forge/plugin-fuses": "^7.8.3",
    "@electron-forge/plugin-webpack": "^7.8.3",
    "@electron/fuses": "^1.8.0",
    "@eslint/js": "9.18.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "@vercel/webpack-asset-relocator-loader": "^1.10.0",
    "babel-loader": "^10.0.0",
    "babel-plugin-import": "^1.13.8",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "css-loader": "^7.1.2",
    "electron": "37.2.6",
    "eslint": "9.18.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.1.0",
    "husky": "9.1.6",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "lint-staged": "15.2.10",
    "node-loader": "^2.1.0",
    "prettier": "3.3.3",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.2.6",
    "ts-loader": "^9.5.2",
    "typescript-eslint": "8.18.0",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --cache --fix --max-warnings=0"
    ],
    "*.{ts,tsx,js,jsx,css,md,json}": [
      "prettier --write"
    ]
  }
}
